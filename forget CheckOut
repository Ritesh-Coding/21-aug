WITH unnested_objects AS (
    SELECT 
        id,
        jsonb_array_elements(title::jsonb) AS obj
    FROM 
        your_table
),
numbered_objects AS (
    SELECT 
        id,
        obj,
        ROW_NUMBER() OVER (ORDER BY id) AS rn
    FROM 
        unnested_objects
)
SELECT 
    id,
    obj->>'name' AS title
FROM 
    numbered_objects
ORDER BY 
    id, rn;



WITH unnested_objects AS (
    SELECT 
        id,
        name
    FROM 
        your_project.your_dataset.your_table,
        UNNEST(title) AS title_struct
),
numbered_objects AS (
    SELECT 
        id,
        name,
        ROW_NUMBER() OVER (ORDER BY id) AS rn
    FROM 
        unnested_objects
)
SELECT 
    id,
    name AS title
FROM 
    numbered_objects
ORDER BY 
    id, rn;

	
